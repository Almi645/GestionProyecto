                    @Html.TextBoxFor(model => model.Proyectos.IP, new { @id = "IP", @placeholder = "Buscar Cliente por Código" })
                </div>
                @Html.ValidationMessageFor(model => model.Proyectos.IP, string.Empty, new { @Class = "error", @Style = "display:block;" })
            </div>
        </div>
        <div class="row-fluid">
            <button id="btnProyectoNuevo" type="button" class="btn btn-info" style="float:right; margin-left:1px; margin-right:1px"> Nuevo </button>
            <button id="btnProyectoBuscar" type="button" class="btn btn-info" style="float:right; margin-left:1px; margin-right:1px"> Buscar </button>
        </div>
    </div>
}
