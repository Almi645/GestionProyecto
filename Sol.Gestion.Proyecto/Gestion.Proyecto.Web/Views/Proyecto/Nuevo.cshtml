@model Gestion.Proyecto.Web.Models.Proyecto.ProyectoConsultaViewModel
@{
    ViewBag.Title = @Gestion.Proyecto.Resource.Title.RegistrarProyecto;
}
@Html.Hidden("UrlNuevoProyecto", Url.Action("Nuevo", "Proyecto"))
@using (Html.BeginForm("ConsultarPartial", "Proyecto", FormMethod.Post, new { @id = "FrmProyectoConsulta" }))
{
    <div class="bs-docs-example">
        <p style="text-transform:uppercase">:: @Gestion.Proyecto.Resource.Title.RegistrarProyecto</p>
    </div>
    <div class="prettyprint">
        <div class="row-fluid">
            <div class="span4">
                @Html.LabelFor(model => model.Proyectos.Codigo, new { @Class = "span3" })
                <div class="span9">
                    @Html.TextBoxFor(model => model.Proyectos.Codigo, new { @id = "Codigo", @placeholder = "Buscar Cliente por Código" })
                </div>
                @Html.ValidationMessageFor(model => model.Proyectos.Codigo, string.Empty, new { @Class = "error", @Style = "display:block;" })
            </div>
            <div class="span4">
                @Html.LabelFor(model => model.Proyectos.Descripcion, new { @Class = "span3" })
                <div class="span9">
                    @Html.TextBoxFor(model => model.Proyectos.Descripcion, new { @id = "Descripcion", @placeholder = "Buscar Cliente por Código" })
                </div>
                @Html.ValidationMessageFor(model => model.Proyectos.Descripcion, string.Empty, new { @Class = "error", @Style = "display:block;" })
            </div>
            <div class="span4">
                @Html.LabelFor(model => model.Proyectos.Estado, new { @Class = "span3" })
                <div class="span9">
                    @Html.TextBoxFor(model => model.Proyectos.Estado, new { @id = "Estado", @placeholder = "Buscar Cliente por Código" })
                </div>
                @Html.ValidationMessageFor(model => model.Proyectos.Estado, string.Empty, new { @Class = "error", @Style = "display:block;" })
            </div>
        </div>
        <div class="row-fluid">
            <div class="span4">
                @Html.LabelFor(model => model.Proyectos.NombreEstacion, new { @Class = "span3" })
                <div class="span9">
                    @Html.TextBoxFor(model => model.Proyectos.NombreEstacion, new { @id = "NombreEstacion", @placeholder = "Buscar Cliente por Código" })
                </div>
                @Html.ValidationMessageFor(model => model.Proyectos.NombreEstacion, string.Empty, new { @Class = "error", @Style = "display:block;" })
            </div>
            <div class="span4">
                @Html.LabelFor(model => model.Proyectos.NombreEquipo, new { @Class = "span3" })
                <div class="span9">
                    @Html.TextBoxFor(model => model.Proyectos.NombreEquipo, new { @id = "NombreEquipo", @placeholder = "Buscar Cliente por Código" })
                </div>
                @Html.ValidationMessageFor(model => model.Proyectos.NombreEquipo, string.Empty, new { @Class = "error", @Style = "display:block;" })
            </div>
            <div class="span4">
                @Html.LabelFor(model => model.Proyectos.TipoEquipo, new { @Class = "span3" })
                <div class="span9">
                    @Html.TextBoxFor(model => model.Proyectos.TipoEquipo, new { @id = "TipoEquipo", @placeholder = "Buscar Cliente por Código" })
                </div>
                @Html.ValidationMessageFor(model => model.Proyectos.TipoEquipo, string.Empty, new { @Class = "error", @Style = "display:block;" })
            </div>
        </div>
        <div class="row-fluid">
            <div class="span4">
                @Html.LabelFor(model => model.Proyectos.ID, new { @Class = "span3" })
                <div class="span9">
                    @Html.TextBoxFor(model => model.Proyectos.ID, new { @id = "ID", @placeholder = "Buscar Cliente por Código" })
                </div>
                @Html.ValidationMessageFor(model => model.Proyectos.ID, string.Empty, new { @Class = "error", @Style = "display:block;" })
            </div>
            <div class="span4">
                @Html.LabelFor(model => model.Proyectos.IP, new { @Class = "span3" })
